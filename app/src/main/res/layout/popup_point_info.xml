<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="24dp"
        android:paddingBottom="18dp">

        <TextView
            android:id="@+id/tvTitle"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/spoqa_han_sans_neo_bold"
            android:text="@string/popup_point_title"
            android:textAlignment="center"
            android:textColor="@color/grey_222"
            android:textSize="18sp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/menuLayout1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/default_side_margin"
            android:layout_marginTop="10dp"
            android:layout_marginRight="@dimen/default_side_margin"
            android:layout_marginBottom="8dp"
            android:background="@drawable/round_withdraw_menu_bg"
            android:paddingTop="12dp"
            android:paddingBottom="12dp"
            app:layout_constraintTop_toBottomOf="@+id/tvTitle">

            <TextView
                android:id="@+id/infoTitle1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="16dp"
                android:drawableLeft="@drawable/img_point"
                android:drawablePadding="5dp"
                android:fontFamily="@font/spoqa_han_sans_neo_bold"
                android:gravity="center"
                android:text="@string/popup_point_sub_title1"
                android:textColor="@color/grey_222"
                android:textSize="13sp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <RelativeLayout
                android:id="@+id/info1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="20dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/infoTitle1">

                <ImageView
                    android:id="@+id/icon1"
                    android:layout_width="2dp"
                    android:layout_height="2dp"
                    android:layout_centerVertical="true"
                    android:background="@color/black" />

                <TextView
                    android:id="@+id/tvDesc1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@+id/icon1"
                    android:fontFamily="@font/spoqa_han_sans_neo_regular"
                    android:includeFontPadding="false"
                    android:textAlignment="textStart"
                    android:textColor="@color/grey_222"
                    android:textSize="14sp"
                    android:textStyle="bold"
                    tools:text="@string/popup_point_desc1" />

            </RelativeLayout>

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="27dp"
                android:layout_marginTop="4dp"
                android:fontFamily="@font/spoqa_han_sans_neo_regular"
                android:text="@string/popup_point_desc2"
                android:textColor="@color/grey_222"
                android:textSize="12sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/info1" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/menuLayout2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/default_side_margin"
            android:layout_marginTop="8dp"
            android:layout_marginRight="@dimen/default_side_margin"
            android:layout_marginBottom="8dp"
            android:background="@drawable/round_withdraw_menu_bg"
            android:paddingTop="12dp"
            android:paddingBottom="12dp"
            app:layout_constraintTop_toBottomOf="@+id/menuLayout1">

            <TextView
                android:id="@+id/infoTitle2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="16dp"
                android:drawableLeft="@drawable/img_point1"
                android:drawablePadding="5dp"
                android:fontFamily="@font/spoqa_han_sans_neo_bold"
                android:gravity="center"
                android:text="@string/popup_point_sub_title2"
                android:textColor="@color/grey_222"
                android:textSize="13sp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <RelativeLayout
                android:id="@+id/info2"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="20dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/infoTitle2">

                <ImageView
                    android:id="@+id/icon2"
                    android:layout_width="2dp"
                    android:layout_height="2dp"
                    android:layout_centerVertical="true"
                    android:background="@color/black" />

                <TextView
                    android:id="@+id/tvDesc2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@+id/icon2"
                    android:fontFamily="@font/spoqa_han_sans_neo_regular"
                    android:includeFontPadding="false"
                    android:textAlignment="textStart"
                    android:textColor="@color/grey_222"
                    android:textSize="14sp"
                    android:textStyle="bold"
                    tools:text="@string/popup_point_desc3" />
            </RelativeLayout>

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="27dp"
                android:layout_marginTop="4dp"
                android:fontFamily="@font/spoqa_han_sans_neo_regular"
                android:text="@string/popup_point_desc4"
                android:textColor="@color/popup_info_text_color"
                android:textSize="12sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/info2" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/menuLayout4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/default_side_margin"
            android:layout_marginTop="8dp"
            android:layout_marginRight="@dimen/default_side_margin"
            android:layout_marginBottom="8dp"
            android:background="@drawable/round_withdraw_menu_bg"
            android:paddingTop="12dp"
            android:paddingBottom="12dp"
            app:layout_constraintTop_toBottomOf="@+id/menuLayout2">

            <TextView
                android:id="@+id/infoTitle4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="16dp"
                android:drawableLeft="@drawable/img_point3"
                android:drawablePadding="5dp"
                android:fontFamily="@font/spoqa_han_sans_neo_bold"
                android:gravity="center"
                android:text="@string/popup_point_sub_title4"
                android:textColor="@color/grey_222"
                android:textSize="13sp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <RelativeLayout
                android:id="@+id/info5"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="20dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/infoTitle4">

                <ImageView
                    android:id="@+id/icon5"
                    android:layout_width="2dp"
                    android:layout_height="2dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerVertical="true"
                    android:layout_marginTop="7dp"
                    android:background="@color/black" />

                <TextView
                    android:id="@+id/tvDesc5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@+id/icon5"
                    android:fontFamily="@font/spoqa_han_sans_neo_regular"
                    android:includeFontPadding="false"
                    android:textAlignment="textStart"
                    android:textColor="@color/grey_222"
                    android:textSize="14sp"
                    android:textStyle="bold"
                    tools:text="@string/popup_point_desc7" />
            </RelativeLayout>

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="27dp"
                android:layout_marginTop="4dp"
                android:fontFamily="@font/spoqa_han_sans_neo_regular"
                android:text="@string/popup_point_desc8"
                android:textColor="@color/popup_info_text_color"
                android:textSize="12sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/info5" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/menuLayout3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/default_side_margin"
            android:layout_marginTop="8dp"
            android:layout_marginRight="@dimen/default_side_margin"
            android:layout_marginBottom="8dp"
            android:background="@drawable/round_withdraw_menu_bg"
            android:paddingTop="12dp"
            android:paddingBottom="12dp"
            app:layout_constraintTop_toBottomOf="@+id/menuLayout4">

            <TextView
                android:id="@+id/infoTitle3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="16dp"
                android:drawableLeft="@drawable/img_point2"
                android:drawablePadding="5dp"
                android:fontFamily="@font/spoqa_han_sans_neo_bold"
                android:gravity="center"
                android:text="@string/popup_point_sub_title3"
                android:textColor="@color/grey_222"
                android:textSize="13sp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <RelativeLayout
                android:id="@+id/info3"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="20dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/infoTitle3">

                <ImageView
                    android:id="@+id/icon3"
                    android:layout_width="2dp"
                    android:layout_height="2dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerVertical="true"
                    android:layout_marginTop="7dp"
                    android:background="@color/black" />

                <TextView
                    android:id="@+id/tvDesc3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@+id/icon3"
                    android:fontFamily="@font/spoqa_han_sans_neo_regular"
                    android:includeFontPadding="false"
                    android:text="@string/popup_point_desc5"
                    android:textAlignment="textStart"
                    android:textColor="@color/grey_222"
                    android:textSize="12sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/info4"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="20dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/info3">

                <ImageView
                    android:id="@+id/icon4"
                    android:layout_width="2dp"
                    android:layout_height="2dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerVertical="true"
                    android:layout_marginTop="7dp"
                    android:background="@color/black" />

                <TextView
                    android:id="@+id/tvDesc4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@+id/icon4"
                    android:fontFamily="@font/spoqa_han_sans_neo_regular"
                    android:includeFontPadding="false"
                    android:text="@string/popup_point_desc6"
                    android:textAlignment="textStart"
                    android:textColor="@color/grey_222"
                    android:textSize="12sp" />
            </RelativeLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>